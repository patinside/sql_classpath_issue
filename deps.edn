                                                   :deps/root "projects/poly"}
                                org.apache.logging.log4j/log4j-api {:mvn/version "2.20.0"}
                                org.apache.logging.log4j/log4j-slf4j-impl {:mvn/version "2.20.0"}}
                   :main-opts ["-m" "polylith.clj.core.poly-cli.core"]}}
